@import '../term/term.wxss';
.box {
  width: 600rpx;
  height: 565rpx;
  position: fixed;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  background-color: #fff;
  border-radius: 10rpx;
  padding: 30rpx;
  box-sizing: border-box;
}

.head {
  height: 55rpx;
  line-height: 55rpx;
  width: 100%;
  font-size: 32rpx;
  text-align: center;
  font-weight: 600;
  overflow: hidden;
}

.img {
  width: 35rpx;
  height: 35rpx;
  float: left;
  margin-top: 10rpx;
}

.text {
  text-align: center;
  font-size: 32rpx;
  margin: 50rpx 0;
  font-weight: 700;
}

.texts {
  margin-bottom: 0;
}

.type {
  width: 100%;
  height: 110rpx;
  border-top: 1px solid #efefef;
  font-size: 24rpx;
}

.type .lt {
  float: left;
  line-height: 110rpx;
}

.type .rt {
  float: right;
  line-height: 110rpx;
}

.lq {
  float: left;
  width: 32rpx;
  height: 32rpx;
  margin-top: 39rpx;
}

.lqt {
  margin: 0 10rpx;
}
.contents{
  width: 100%;
  box-sizing: border-box;
  display: flex;
  justify-content: space-around;
  align-items: center;
  margin-top: 50rpx;
}

.iptbox{
  flex: 1;
  height: 80rpx;
  border:1rpx solid #ddd;
  box-sizing: border-box;
  display: flex;
  justify-content: center;
  align-items: center;
  text-align: center;
}
.ipt_s{
  width: 0;
  height: 0;
}




