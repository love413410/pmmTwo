page {
  background-color: #efefef;
  padding-bottom: 100rpx;
}

.list {
  margin: 10rpx 0;
}

.item {
  width: 100%;
  padding-left: 30rpx;
  box-sizing: border-box;
  height: 88rpx;
  line-height: 88rpx;
  background-color: #fff;
  font-size: 28rpx;
}

.itm {
  width: 100%;
  height: 100%;
  padding-right: 75rpx;
  box-sizing: border-box;
  border-bottom: 1px solid #efefef;
  position: relative;
}

.itm .lt {
  width: 30%;
}

.itm .rt {
  width: 60%;
}
.rt_r{
  float: right;
  color: #808080;
}
.rt_r image{
  position: absolute;
  right:39rpx;
  top: 30rpx;
  width: 26rpx;
  height: 26rpx;
}
.itm .code{
  width: 60%;
}
.itm .codeBtn{
  width: 40%;
  text-align: right;
  color: #fb7313;
}
.itm .rt input {
  width: 100%;
  height: 88rpx;
  text-align: right;
  color: #b2b2b2;
}
.itm .lt input{
  width: 100%;
  height: 88rpx;
  color: #b2b2b2;
}

.up {
  width: 100%;
  padding: 30rpx;
  box-sizing: border-box;
  background-color: #fff;
  overflow: hidden;
  margin-bottom: 10rpx;
}

.up-img {
  width: 190rpx;
  height: 240rpx;
}

.up-img image {
  width: 100%;
  height: 100%;
}

.up .text {
  width: 400rpx;
  height: 240rpx;
  margin-left: 60rpx;
}

.tle {
  font-size: 30rpx;
  color: #333;
  height: 90rpx;
  line-height: 90rpx;
}

.conte {
  font-size: 20rpx;
  color: #999;
}
.contes{
  font-size: 20rpx;
  color: #fc612b;
}
.sub-btn{
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  background-color: #fb7313;
  color: #fff;
  font-size: 36rpx;
}

.not_maskBox {
  width: 600rpx;
  height: 800rpx;
  padding-bottom: 100rpx;
  background-color: #fff;
  position: fixed;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  box-sizing: border-box;
  border-radius: 10rpx;
  position: relative;
}
.mask_tle{
  width: 100%;
  height: 200rpx;
  border-bottom:1px solid #efefef;
  text-align: center;
}
.mask_tle view{
  height: 100rpx;
}
.tle1{
  font-size: 36rpx;
  line-height: 180rpx;
}
.tle2{
  font-size: 24rpx;
  line-height: 80rpx;
  color: #b9b9b9;
}


.info {
  width: 100%;
  height: 100%;
  padding: 0 30rpx;
  box-sizing: border-box;
  border-top-left-radius: 10rpx;
  border-top-right-radius: 10rpx;
}
.info-item {
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  border-bottom: 1px solid #efefef;
  font-size: 24rpx;
}

.send {
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  color: #fff;
  background-color: #fb7313;
  border-bottom-left-radius: 10rpx;
  border-bottom-right-radius: 10rpx;
}

.idx {
  width: 40rpx;
}

.info .text {
  margin-left: 50rpx;
}
.qxa{
  width: 80rpx;
  height: 80rpx;
  position: absolute;
  right: 0;
  top: 0;
}