page{
  background-color: #efefef;
  padding-bottom: 100rpx;
}
.item{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  border-bottom: 1px solid #efefef;
  background-color: #fff;
  padding-left: 50rpx; 
  padding-right: 60rpx;
  box-sizing: border-box; 
  font-size: 28rpx;
}
.lt{
  color: #8d8d8d;
}
.rt{
  color: #5d5d5d;
}
.pic{
  width: 100%;
  height: 250rpx;
  padding-left: 50rpx; 
  box-sizing: border-box;
  background-color: #fff;
  font-size: 28rpx;
  color: #8d8d8d;
  border-bottom: 1px solid #efefef;
}
.pic-tle{
  height: 100rpx;
  line-height: 100rpx;
}
.pics{
  width: 100%;
  height: 120rpx;
  background-color: #fff;
  color: #8b8b8b;
}

.pics image{
  width: 120rpx;
  height: 120rpx;
  margin-right: 10rpx;
}
.desc{
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  padding-left: 50rpx; 
  box-sizing: border-box;
  background-color: #fff;
  font-size: 28rpx;
  color: #8d8d8d;
}
.items{
  width: 100%;
  line-height: 45rpx;
  background-color: #fff;
  padding-left: 50rpx; 
  padding-right: 60rpx;
  box-sizing: border-box; 
  font-size: 28rpx;
}
.foot-btn{
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  color: #fff;
  background-color: #fb7313;
}

.not_maskBox{
  width: 600rpx;
  height: 800rpx;
  background-color: #fff;
  position: fixed;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  position: relative;
  box-sizing: border-box;
  border-radius: 10rpx;
}
.qxa{
  width: 64rpx;
  height: 64rpx;
  position: absolute;
  left: 50%;
  margin-left: -32rpx;
  bottom: -105rpx;
}
.info{
  width: 100%;
  height: 700rpx;
  overflow-y: auto;
  padding: 0 30rpx;
  box-sizing: border-box;
  border-top-left-radius: 10rpx;
  border-top-right-radius: 10rpx;
}
.send{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  color: #fff;
  background-color: #fb7313;
  border-bottom-left-radius: 10rpx;
  border-bottom-right-radius: 10rpx;
}
.info-item{
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  border-bottom: 1px solid #efefef;
  font-size: 24rpx;
}
.idx{
  width: 40rpx;
}
.text{
  margin-left: 50rpx;
}