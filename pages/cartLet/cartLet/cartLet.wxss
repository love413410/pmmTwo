.tle-head {
  width: 100vw;
  height: 130rpx;
  line-height: 90rpx;
  position: fixed;
  z-index: 5000;
  top: 0;
  left: 0;
  padding-top: 40rpx;
  box-sizing: border-box;
  background-color: #fff;
}

.tle-tle {
  width: 600rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.tle-nyw{
  width: 750rpx;
}


page {
  background-color: #efefef;
  padding-top: 130rpx;
  box-sizing: border-box;
}

.head {
  width: 100%;
  height: 220rpx;
  background-color: #fff;
  padding: 45rpx 0;
  box-sizing: border-box;
}

.head image {
  width: 130rpx;
  height: 130rpx;
  margin-left: 50rpx;
  margin-right: 30rpx;
}

.info {
  font-size: 36rpx;
  color: #1a1a1a;
}

.top {
  height: 65rpx;
  line-height: 100rpx;
}

.btm {
  height: 65rpx;
  line-height: 65rpx;
  font-size: 22rpx;
  color: #a7a7a7;
}

.item {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  padding: 0 30rpx;
  margin-top: 10rpx;
  background-color: #fff;
  font-size: 28rpx;
  position: relative;
  box-sizing: border-box;
}

.num {
  padding: 0 10rpx;
  line-height: 30rpx;
  text-align: center;
  margin-right: 40rpx;
  margin-top: 35rpx;
  font-size: 20rpx;
  color: #fff;
  background-color: #ed3a3a;
  border-radius: 20rpx;
}

.img {
  width: 14rpx;
  height: 22rpx;
  position: absolute;
  right: 30rpx;
  top: 50%;
  transform: translateY(-50%);
}
.img-t {
  transform: rotate(90deg);
}

.hideItem {
  width: 100%;
  height: 185rpx;
  background-color: #fff;
  padding-left: 30rpx;
  box-sizing: border-box;
}

.hide {
  width: 100%;
  height: 100%;
  border-top: 1px solid #efefef;
}
.hide .lt{
  width: 50%;
  height: 100%;
  text-align: center;
  font-size: 24rpx;
  color: #676767;
  position: relative;
}
.hide-img{
  width: 48rpx;
  height: 48rpx;
  display: inline-block;
  margin-top: 45rpx;
  margin-bottom: 25rpx;
}
.hide-img image{
  display: block;
  width: 100%;
  height:100%;
}
.w_num{
  position: absolute;
  left: 180rpx;
  top: 30rpx;
  padding: 0 10rpx;
  line-height: 30rpx;
  text-align: center;
  font-size: 20rpx;
  color: #fff;
  background-color: #ed3a3a;
  border-radius: 20rpx;
}