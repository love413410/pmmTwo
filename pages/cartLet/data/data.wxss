page {
  background-color: #efefef;
  padding-bottom: 100rpx;
}

.item {
  width: 100%;
  height: 200rpx;
  padding: 0 30rpx;
  box-sizing: border-box;
  margin-top: 20rpx;
}

.itemBox {
  width: 100%;
  height: 100%;
  background-color: #fff;
  border-radius: 8rpx;
  padding: 40rpx;
  box-sizing: border-box;
}

.img {
  width: 110rpx;
  height: 110rpx;
  margin-top: 5rpx;
}

.item-text {
  width: 380rpx;
  padding-left: 40rpx;
  box-sizing: border-box;
  font-size: 24rpx;
  color: #515151;
  line-height: 40rpx;
}
.item-text>view{
  overflow: hidden;
}
.text .lt {
  color: #808080;
}

.btn{
  width: 100rpx;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
  color: #f17171;
  border:1px solid #f17171;
  border-radius: 15rpx;
  font-size: 20rpx;
}

.not_maskBox{
  width: 600rpx;
  height: 800rpx;
  background-color: #fff;
  position: fixed;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  position: relative;
  box-sizing: border-box;
  border-radius: 10rpx;
}


.qxa{
  width: 64rpx;
  height: 64rpx;
  position: absolute;
  left: 50%;
  margin-left: -32rpx;
  bottom: -105rpx;
}
.info{
  width: 100%;
  height: 700rpx;
  overflow-y: auto;
  padding: 0 30rpx;
  box-sizing: border-box;
  border-top-left-radius: 10rpx;
  border-top-right-radius: 10rpx;
}
.send{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  color: #fff;
  background-color: #fb7313;
  border-bottom-left-radius: 10rpx;
  border-bottom-right-radius: 10rpx;
}
.info-item{
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  border-bottom: 1px solid #efefef;
  font-size: 24rpx;
}
.idx{
  width: 40rpx;
}
.text{
  margin-left: 50rpx;
}
.footBtn {
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  background-color: #fb7313;
  color: #fff;
  font-size: 36rpx;
  border-radius: 0;
}