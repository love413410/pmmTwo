page {
  background-color: #efefef;
  padding-bottom: 95rpx;
}

.top {
  width: 100%;
  height: 100%;
  background-color: #fff;
}

.top_img {
  width: 750rpx;
  height: 750rpx;
  margin: auto;
}
.top_img swiper {
  width: 100%;
  height: 100%;
}

.top_img swiper swiper-item {
  width: 100%;
  height: 100%;
}


.top_img image {
  width: 100%;
  height: 100%;
}

.name {
  width: 100%;
  height: 40rpx;
  line-height: 40rpx;
  text-indent: 48rpx;
  font-size: 30rpx;
  color: #fb7313;
  padding-top: 30rpx;
}

.shop {
  text-indent: 0rpx;
  padding: 0 10rpx;
  height: 40rpx;
  text-align: center;
  font-size: 20rpx;
  background-color: #26fb7313;
  margin-left: 20rpx;
  border-radius: 4rpx;
}

.text {
  padding: 30rpx 48rpx;
  box-sizing: border-box;
  font-size: 30rpx;
  line-height: 40rpx;
  color: #333;
  background-color: #fff;
  margin-bottom: 20rpx;
}

.duihuan {
  width: 100%;
  height: 95rpx;
  line-height: 95rpx;
  text-align: center;
  color: #fff;
  font-size: 32rpx;
  background-color: #fb7313;
  position: fixed;
  left: 0;
  bottom: 0;
}
.shop_box {
  width: 550rpx;
  height: 565rpx;
  position: fixed;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  background-color: #fff;
  border-radius: 10rpx;
  overflow: hidden;
}

.shop_top {
  width: 100%;
  height: 285rpx;
  position: relative;
}

.shop_top .bg {
  width: 100%;
  height: 100%;
}

.shop_top .bg2 {
  width: 80rpx;
  height: 80rpx;
  position: absolute;
  right: 0;
  top: 0;
}
.shop_b{
  width: 100%;
  height: 280rpx;
}
.shop_text{
  font-size: 24rpx;
  line-height: 40rpx;
  color: #afafaf;
  width: 260rpx;
  margin: auto;
  text-align: center;
  margin-top: 50rpx;
  margin-bottom: 50rpx;
}
.shop_b .btn{
  width: 310rpx;
  height:65rpx;
  line-height: 65rpx;
  text-align: center;
  color: #fff;
  border-radius: 8rpx;
  background-color: #ff921d;
  margin: auto;
  font-size: 28rpx;
}
.dateVal{
  background:#fff;
  padding: 0 35rpx;
  box-sizing: border-box;
}