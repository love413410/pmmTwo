@import '../rewa/rewa.wxss';
.frBox {
  width: 548rpx;
  height: 556rpx;
  position: fixed;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  background-color: #fff;
  border-radius: 10rpx;
}

.frBox image.c {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
}

.frQx {
  position: absolute;
  top: 0;
  right: 0;
  width: 80rpx;
  height: 80rpx;
}

.frQx image {
  width: 100%;
  height: 100%;
}

.r_tle {
  height: 90rpx;
  line-height: 90rpx;
  text-align: center;
  font-size: 32rpx;
  color: #ff921d;
}

.frbtn {
  width: 330rpx;
  height: 70rpx;
  position: absolute;
  bottom: 40rpx;
  left: 50%;
  transform: translateX(-50%);
}
