page {
  background-color: #fff;
}
.head {
  width: 100%; 
  height: 250rpx;
  position: relative;
}

.banner {
  width: 100%;
  height: 250rpx;
}
.top_swiper{
  width: 100%;
  height: 100%;
}
.head swiper {
  width: 100%;
  height: 100%;
}

.head swiper swiper-item {
  width: 100%;
  height: 100%;
}

.head image {
  width: 100%;
  height: 100%;
}

.heads {
  background-color: #fff;
  padding: 0 25rpx;
  box-sizing: border-box;
  color: #a1a1a1;
  padding-bottom: 40rpx;
}
.top_ {
  height: 75rpx;
  font-size: 30rpx;
  line-height: 105rpx;
  color: #5d5d5d;
}

.btm_ {
  height: 75rpx;
  font-size: 24rpx;
  line-height: 55rpx;
  color: #999;
}

.tle {
  width: 100%;
  height: 42rpx;
  border-bottom: 1px solid #efefef;
}

.tle view {
  float: left;
  width: 25%;
  text-align: center;
}
.tle view:first-child{
  text-align: left;
}

.box {
  width: 100%;
  height: 100%;
}

.li {
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  border-bottom: 1px solid #efefef;
  overflow: hidden;
}

.li view {
  float: left;
  width: 25%;
  text-align: center;
}
.li view:first-child{
  text-align: left;
  text-indent: 25rpx;
}
.num{
  color: #333;
}

.purc{
  width: 110rpx;
  height: 64rpx;
  line-height: 64rpx;
  text-align: center;
  color: #fff;
  background-color: #fb7313;
  border-radius: 10rpx;
  margin-top: 13rpx;
}
.dibu{
  width: 100%;
  background-color: #fff;
  text-indent: 20rpx;
  font-size: 24rpx;
  line-height: 50rpx;
  color: #999999;
}
.top{
  width: 100%;
  height: 15rpx;
  background-color: #efefef;
  
}
.title{
  height: 60rpx;
  line-height: 84rpx;
  color: #666666;
}
.mask_box{
  width: 550rpx;
  height: 550rpx;
  background-color: #fff;
  border-radius: 8rpx;
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  margin: auto;
  
}
.img{
  float: right;
  width: 80rpx;
  height: 80rpx;
}
.mask_tle{
  width: 100%;
  height: 135rpx;
  line-height: 135rpx;
  text-align: center;
  font-size: 32rpx;
  color: #fe7940;
  text-indent: 80rpx;
  font-weight: 600;
}
.type{
  font-size: 24rpx;
  color: #b0b0b0;
  line-height: 24rpx;
  text-indent: 30rpx;
}
.style{
  width: 100%;
  height: 60rpx;
  margin: 45rpx 0;
  text-align: center;
}
.style view{
  width: 110rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  display: inline-block;
  background-color: #eeeeee;
}

.num{
  margin: 0 5rpx;
}
.style input{
  width: 110rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  display: inline-block;
  background-color: #eeeeee;
  vertical-align: top;
}
.redu{
  border-top-left-radius: 8rpx;
  border-bottom-left-radius: 8rpx;
}
.add{
  border-top-right-radius: 8rpx;
  border-bottom-right-radius: 8rpx;
}
.types{
  margin-bottom: 25rpx;
}
.sub_btm{
  width: 310rpx;
  height: 65rpx;
  line-height: 65rpx;
  text-align: center;
  color: #fff;
  background-color: #ff921d;
  border-radius: 8rpx;
  margin: auto;
  margin-top: 80rpx;
}