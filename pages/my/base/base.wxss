page{
  background-color: #efefef;
  padding-bottom: 95rpx;
  box-sizing: border-box;
}
.item{
  width: 100%;
  padding-left: 30rpx;
  box-sizing: border-box;
  background-color: #fff;
}
.item-box{
  width: 100%;
  height: 120rpx;
  border-bottom: 1px solid #efefef;
}
.item-box image{
  width: 78rpx;
  height: 70rpx;
  margin-top: 25rpx;
  margin-right: 35rpx;
}
.center{
  width: 500rpx;
}
.tle{
  height: 60rpx;
  font-size: 32rpx;
  line-height: 90rpx; 
  color: #303442;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.date{
  height: 60rpx;
  font-size: 30rpx;
  line-height: 60rpx;
  color: #7d838b; 
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.share{
  width: 100%;
  height: 95rpx;
  line-height: 95rpx;
  text-align: center;
  background-color: #ef7e20;
  color: #fff;
  position: fixed;
  left: 0;
  bottom: 0;
}
.check{
  line-height: 120rpx;
  margin-right: 30rpx;
} 