page {
  background-color: #efefef;
  padding-bottom: 95rpx;
  box-sizing: border-box;
}

.head {
  width: 100%;
  height: 150rpx;
  line-height: 110rpx;
  padding: 20rpx 30rpx;
  box-sizing: border-box;
  background-color: #fff;
  margin-bottom: 10rpx;
  color: #333;
  font-size: 32rpx;
}

.head-item {
  width: 330rpx;
  height: 100%;
  background-color: #f7f7f7;
  border-radius: 8rpx;
}

.head-item .img {
  width: 62rpx;
  height: 62rpx;
  vertical-align: middle;
  margin-left: 30rpx;
  margin-right: 20rpx;
  display: inline-block;
}

.text {
  display: inline-block;
}

.item {
  width: 100%;
  padding-left: 30rpx;
  box-sizing: border-box;
  background-color: #fff;
}

.item-box {
  width: 100%;
  height: 120rpx;
  border-bottom: 1px solid #efefef;
}

.imgBoxs {
  width: 78rpx;
  height: 70rpx;
  margin-top: 25rpx;
  margin-right: 35rpx;
  position: relative;
}

.imgBoxs image {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
}

.form {
  position: absolute;
  left: 0;
  bottom: 12rpx;
  width: 100%;
  height: 22rpx;
  line-height: 22rpx;
  font-size: 20rpx;
  text-align: center;
  color: #fff;
}

.f1 {
  background-color: #efcb35;
}

.f2 {
  background-color: #71a2e2;
}

.center {
  width: 500rpx;
}

.tle {
  height: 60rpx;
  font-size: 32rpx;
  line-height: 85rpx;
  color: #303442;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.date {
  float: left;
  height: 60rpx;
  font-size: 30rpx;
  line-height: 60rpx;
  color: #7d838b;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.share {
  width: 100%;
  height: 95rpx;
  line-height: 95rpx;
  text-align: center;
  background-color: #ef7e20;
  color: #fff;
  position: fixed;
  left: 0;
  bottom: 0;
  border-radius: 0; 
}

.check {
  line-height: 120rpx;
  margin-right: 30rpx;
}

.share2 {
  display: flex;
}
.share2 button{
  flex: 1;
  background-color: #ef7e20;
  border-radius: 0;
  color: #fff;
}
.up{
  border-right:1px solid #fff;
  box-sizing: border-box;
}
.share2 button::after{
  border: none;
}