page {
  background-color: #f3f3f3;
}

.item {
  width: 100%;
  height: 100%;
  background-color: #fff;
  margin-top: 15rpx;
  padding: 0 40rpx;
  box-sizing: border-box;
  overflow: hidden;
}
 
.top {
  width: 100%;
  height: 300rpx;
  overflow: hidden;
  border-bottom: 1px solid #e5e5e5;
  box-sizing: border-box;
}

.imgBox {
  width: 100rpx;
  height: 100rpx;
  margin: auto;
  margin-top: 40rpx;
}

.imgBox image {
  display: block;
  width: 100%;
  height: 100%;
}

.name {
  width: 100%;
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
  color: #4d4d4d;
  font-weight: 600;
}

.num {
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  text-align: center;
  color: #4d4d4d;
  font-weight: 600;
  font-size: 50rpx;
}
.bot{
  width: 100%;
  padding: 55rpx 0;
  box-sizing: border-box;

}
.list{
  width: 100%;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 28rpx;
  color: #b3b3b3;
  margin-top:5rpx; 
}
.list text{
  color:#6e6e6e;
  font-weight: 600;
  margin-left: 45rpx;
}
.bot .list .chakan{
  color: #ef7e20;
}