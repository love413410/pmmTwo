@import '../../home/releDeta/releDeta.wxss';

page {
  background-color: #efefef;
  position: relative;
  padding-bottom: 100rpx;
}

.head {
  width: 100%;
  padding-left: 30rpx;
  box-sizing: border-box;
  background-color: #fff;
}

.head view {
  border-bottom: 1px solid #efefef;
  font-size: 32rpx;
  color: #373737;
  height: 100rpx;
  line-height: 100rpx;
}

.list {
  width: 100%;
  padding-left: 30rpx;
  box-sizing: border-box;
  background-color: #fff;
  border-bottom: 1px solid #efefef;
  margin-bottom: 20rpx;
}

.list .items {
  width: 100%;
  height: 125rpx;
  border-bottom: 1px solid #efefef;
  padding: 30rpx 0;
  box-sizing: border-box;
  line-height: 65rpx;
  font-size: 24rpx;
  display: flex;
  justify-content: space-between;
}

.list .items image {
  width: 65rpx;
  height: 65rpx;
}

.name {
  color: #2f2f2f;
  font-weight: 700;
  margin: 0 15rpx;
}

.red {
  color: #ef4c4c;
}

.green {
  color: #08a221;
  margin: 0 30rpx;
}

.mode {
  width: 660rpx;
  height: 430rpx;
  position: relative;
  margin: auto;
  margin-top: 50%;
}
.qx{
  width: 75rpx;
  height: 75rpx;
  position: absolute;
  left: 50%;
  bottom: -150rpx;
  transform:translateX(-50%);
  z-index: 99999;
}

.top {
  height: 330rpx;
  width: 100%;
  background-color: #fff;
  position: relative;
  border-top-left-radius: 10rpx;
  border-top-right-radius: 10rpx;
}
.top image{
  width: 150rpx;
  height: 150rpx;
  position: absolute;
  top: 50rpx;
  left: 50%;
  transform:translateX(-50%);
}
.text{
  width: 100%;
  font-size: 32rpx;
  color:#434343;
  text-align: center;
  font-weight: 700;
  position: absolute;
  bottom: 40rpx;
  left: 50%;
  transform:translateX(-50%);
}


.btm {
  width: 100%;
  height: 100rpx;
  background-color: #22ac38;
  text-align: center;
  line-height: 100rpx;
  color: #fff;
  font-size: 40rpx;
  border-bottom-left-radius: 10rpx;
  border-bottom-right-radius: 10rpx;
}



.footBtn {
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  color: #fff;
  font-size: 36rpx;
  border-radius: 0;
  background-color: #fb7313;
  z-index: 999;
}

