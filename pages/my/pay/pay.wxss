page{
  background-color: #f3f3f3;
}
.head{
  width: 100%;
  height: 300rpx;
  background: linear-gradient(#f4d63a, #ef8121);
  padding: 0 5%;
  box-sizing: border-box;
  color: #fff;
} 
.head .item{
  width: 100%;
  height: 50%;
  line-height: 150rpx;
  display: flex;
  justify-content: space-between;
  font-size: 32rpx;

}
.pay{
  font-size: 50rpx;
}
.pr{
  width: 150rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  border:3rpx solid #fff;
  border-radius: 10rpx;
  margin-top: 45rpx;
  margin-right: 20rpx;
}
.tle{
  width: 100%;
  height: 160rpx;
  padding: 0 35rpx;
  box-sizing: border-box;
}
.tle-top{
  width: 100%;
  height: 50%;
  font-size: 30rpx;
  line-height: 100rpx;
  color: #333333;
}
.tle-top view{
  display: inline-block;
  margin-right: 20rpx;
}
.tle-top image{
  width: 20rpx;
  height: 12rpx;
}
.tle-bot{
  font-size: 28rpx;
  line-height: 80rpx;
  color: #808080;
}
.box {
  width: 100%;
  height: 100%;
  background-color: #fff;
  padding-left: 30rpx;
  box-sizing: border-box;
  padding-bottom:30rpx; 
  
} 

.box .item {
  width: 100%;
  height: 160rpx;
  border-bottom: 1rpx solid #e5e5e5;
  padding: 40rpx 0;
  box-sizing: border-box;
  display: flex;
}
.imgBox{
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  overflow: hidden;
}
.imgBox image{
  display: block;
  width: 100%;
  height: 100%;
}
.item-te{
  width: 590rpx;
  height: 40rpx;
  line-height: 40rpx;
  display: flex;
  justify-content: space-between;
  padding-left:30rpx;
  box-sizing: border-box; 
  color: #000;
  font-size: 30rpx;
} 
.item-ti{
  width: 590rpx;
  height: 45rpx;
  line-height: 65rpx;
  padding-left:30rpx;
  box-sizing: border-box; 
  color: #808080;
  font-size: 24rpx;
}
