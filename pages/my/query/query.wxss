page {
  background-color: #efefef;
}

.head-nav {
  width: 100%;
  height: 200rpx;
  background-color: #fff;
  padding: 20rpx 30rpx;
  box-sizing: border-box;
}

.head-nav navigator {
  width: 325rpx;
  height: 100%;
  border-radius: 10rpx;
  position: relative;
}

.head-nav navigator.lt {
  background-color: #eff8ff;
}

.head-nav navigator.rt {
  background-color: #fcf5d7;
}
navigator image.lt{
  width: 66rpx;
  height: 74rpx;
  margin: 0 20rpx;
  margin-top: 43rpx;
}

.text view {
  height: 80rpx;
  font-size: 34rpx;
  color: #333;
  line-height: 114rpx;
}

.text .btm {
  font-size: 20rpx;
  line-height: 80rpx;
}

.item-head {
  width: 100%;
  height: 58rpx;
  line-height: 58rpx;
  margin: 20rpx 0;
  font-size: 24rpx;
  color: #333;
  padding: 0 27rpx;
  box-sizing: border-box;
}

.sub {
  width: 220rpx;
  height: 100%;
  text-align: center;
  background-color: #fff;
  border-radius: 8rpx;
  margin-right: 18rpx;
  padding: 0 17rpx;
  box-sizing: border-box;
}

.subs {
  z-index: 99;
  width: 220rpx;
  height: 100%;
  text-align: center;
  background-color: #fff;
  margin-right: 18rpx;
  border-top-left-radius: 10rpx;
  border-top-right-radius: 10rpx;
  position: relative;
  padding: 0 17rpx;
  box-sizing: border-box;
}

.sb {
  margin-right: 0;
}

.tle_ {
  width: 160rpx;
  float: left;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.item-head image {
  width: 16rpx;
  height: 9rpx;
  float: right;
  margin-left: 10rpx;
  margin-bottom: 5rpx;
  margin-top: 24rpx;
}

.item {
  width: 100%;
  padding: 0 30rpx;
  box-sizing: border-box;
  margin-bottom: 20rpx;
}

.item_box {
  width: 100%;
  background-color: #fff;
  border-radius: 8rpx;
  padding: 35rpx 35rpx 35rpx 145rpx;
  box-sizing: border-box;
  position: relative;
  overflow: hidden;
}

.item_box image.lt {
  width: 110rpx;
  height: 110rpx;
  position: absolute;
  left: 35rpx;
  top: 50%;
  transform: translateY(-50%);
}

.item_box view.box {
  width: 350rpx;
  line-height: 40rpx;
  font-size: 24rpx;
  color: #808080;
  margin-left: 40rpx;
}

.item_box text {
  color: #515151;
}
.item_box .bohao{
  position: absolute;
  right: 35rpx;
  top: 50%;
  transform: translateY(-50%);
  width: 100rpx;
  font-size: 24rpx;
  color: #22ac38;
  text-align: center;
}

.item_box image.dh {
  width: 60rpx;
  height: 60rpx;
}