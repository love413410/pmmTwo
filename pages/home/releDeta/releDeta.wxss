.item {
  width: 100%;
  height: 105rpx;
  line-height: 105rpx;
  display: flex;
  justify-content: space-between;
  padding: 0 35rpx;
  box-sizing: border-box;
  background-color: #fff;
  margin-bottom: 1px;
  font-size: 30rpx;
  color: #8b8b8b;
}

.r {
  color: #2c2c2c;
}

.pics {
  width: 100%;
  line-height: 105rpx;
  padding: 0 35rpx;
  box-sizing: border-box;
  background-color: #fff;
  margin-bottom: 1px;
  font-size: 30rpx;
  color: #8b8b8b;
  padding-bottom: 25rpx;
}

.pics image {
  width: 120rpx;
  height: 120rpx;
  margin-right: 10rpx;
}

.pic {
  width: 100%;
  height: 70rpx;
  line-height: 70rpx;
  padding: 0 35rpx;
  box-sizing: border-box;
  background-color: #fff;
  font-size: 30rpx;
  color: #8b8b8b;
}

.desc {
  width: 100%;
  padding: 0 35rpx;
  background-color: #fff;
  font-size: 35rpx;
  padding-bottom: 25rpx;
  box-sizing: border-box;
  word-wrap: break-word;
  word-break: normal;
}

page {
  padding-bottom: 100rpx;
  background-color: #efefef;
}

.sup {
  position: relative;
}

.sup::after {
  content: '2';
  position: absolute;
  right: -17rpx;
  top: -5rpx;
}

.footBtn {
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  background-color: #fb7313;
  color: #fff;
  font-size: 36rpx;
  border-radius: 0;
  z-index: 2;
}

.footBtn button{
  float: left;
  width: 50%;
  height: 100%;
  border-radius: 0;
  background-color: #fb7313;
  color: #fff;
  box-sizing: border-box;
}
.rtBtn{
  border-left: 1px solid #fff;
}
.ltBtn{
  border-right: 1px solid #fff;
}

.bg1 {
  background-color: #fb7313;
}

.bg2 {
  background-color: #808080;
}

.bg3 {
  background-color: #22ac38;
}
.bg4{
  background-color: #039be5;
}


.rewa {
  width: 550rpx;
  height: 560rpx;
  background-color: #fff;
  border-radius: 10rpx;
  overflow: hidden;
  position: fixed;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
}

.rew_top {
  width: 100%;
  height: 300rpx;
}
 .rew_top image {
  display: block;
  width: 100%;
  height: 100%;
  position: relative;
}
.move{ 
  width: 80rpx;
  height: 80rpx;
  position: absolute;
  right: 0;
  top: 0;
}
.rew_btm {
  width: 100%;
  height: 260rpx;
}

.top_text {
  width: 380rpx;
  font-size: 26rpx;
  color: #999;
  text-align: center;
  margin: auto;
  margin-top: 20rpx;

}

.rouTo {
  width: 306rpx;
  height: 65rpx;
  line-height: 65rpx;
  text-align: center;
  color: #fff;
  background-color: #ff921d;
  font-size: 26rpx;
  border-radius: 10rpx;
  margin: auto;
  margin-top: 35rpx;
}
