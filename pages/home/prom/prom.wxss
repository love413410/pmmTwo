page {
  background-color: #efefef;
}

.head {
  width: 100%; 
  height: 250rpx;
  position: relative;
}

.banner {
  width: 100%;
  height: 250rpx;
}
.top_swiper{
  width: 100%;
  height: 100%;
}
.head swiper {
  width: 100%;
  height: 100%;
}

.head swiper swiper-item {
  width: 100%;
  height: 100%;
}

.head image {
  width: 100%;
  height: 100%;
}

.nav-head{
  width: 100%;
  height: 150rpx;
  background-color: #fff;
  padding: 25rpx 35rpx;
  box-sizing: border-box;
  
}
.navTo{
  width: 325rpx;
  height: 100rpx;
  line-height: 58rpx;
  background-color: #f7f7f7;
  padding: 21rpx 28rpx;
  box-sizing: border-box;
  font-size: 30rpx;
}
.nav-img{
  width: 58rpx;
  height: 58rpx;
  margin-right: 22rpx;
}

.list {
  width: 100%;
  padding: 0 27rpx;
  box-sizing: border-box;
  position: relative;
  margin-top: 20rpx;
}

.item-head {
  width: 100%;
  height: 58rpx;
  line-height: 58rpx;
  margin: 20rpx 0;
  font-size: 24rpx;
  color: #333;
}

.sub {
  width: 220rpx;
  height: 100%;
  text-align: center;
  background-color: #fff;
  border-radius: 8rpx;
  margin-right: 18rpx;
  padding: 0 17rpx;
  box-sizing: border-box;
}

.subs {
  z-index: 99999;
  width: 220rpx;
  height: 100%;
  text-align: center;
  background-color: #fff;
  margin-right: 18rpx;
  border-top-left-radius: 10rpx;
  border-top-right-radius: 10rpx;
  position: relative;
  padding: 0 17rpx;
  box-sizing: border-box;
}
.sb {
  margin-right: 0;
}

.tle_ {
  width: 160rpx;
  float: left;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.item-head image {
  width: 16rpx;
  height: 9rpx;
  float: right;
  margin-left: 10rpx;
  margin-bottom: 5rpx;
  margin-top: 24rpx;
}

.item {
  width: 100%;
  height: 100%;
  border-radius: 10rpx;
  background-color: #fff;
  margin-bottom: 20rpx;
}

.top {
  width: 100%;
  height: 76rpx;
  padding: 10rpx 30rpx;
  padding-right: 40rpx;
  box-sizing: border-box;
  line-height: 56rpx;
  border-bottom: 1px solid #efefef;
  position: relative;
}

.top .state {
  width: 74rpx;
  height: 74rpx;
  position: absolute;
  right: 0rpx;
  top: 0rpx;
  margin-right: 0;
}

.top image {
  width: 44rpx;
  height: 44rpx;
  margin-right: 10rpx;
}

.top .name {
  font-size: 28rpx;
  color: #282828;
  margin-left: 15rpx;
  margin-right: 20rpx;
}

.renzImg {
  width: 100rpx;
  height: 40rpx;
  line-height: 0;
  margin-top: 8rpx;
  margin-right: 20rpx;
}

.vipImg {
  width: 40rpx;
  height: 44rpx;
  line-height: 0;
  margin-top: 6rpx;
}

.renzImg image, .vipImg image {
  width: 100%;
  height: 100%;
}

.top .rt {
  font-size: 24rpx;
  color: #b3b3b3;
}

.item-t {
  width: 100%;
  /* height: 170rpx; */
  height: 200rpx;
  position: relative;
  border-bottom: 1px dotted #eaeaea;
  padding: 30rpx 40rpx;
  box-sizing: border-box;
}

.item-t>.lt {
  width: 440rpx;
}

.item-t .tle {
  height: 70rpx;
  line-height: 70rpx;
  font-size: 36rpx;
  font-weight: 600;
  color: #282828;
}

.item-t .add {
  height: 70rpx;
  line-height: 70rpx;
  font-size: 28rpx;
}

.item-t .add image {
  width: 20rpx;
  height: 26rpx;
  margin-right: 20rpx;
  margin-top: 23rpx;
}

.item-t .add view{
  width: 400rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.item-t .rt {
  width: 100rpx;
  height: 100rpx;
  margin-top: 20rpx;
}

.item-b {
  width: 100%;
  height: 95rpx;
  position: relative;
  padding: 15rpx 40rpx;
  box-sizing: border-box;
  display: flex;
}

.item-b .ltext {
  width: 100rpx;
  word-wrap: break-word;
  word-break: normal;
  font-size: 26rpx;
  line-height: 32rpx;
}

.item-b .rtext {
  width: 540rpx;
  word-wrap: break-word;
  word-break: normal;
  font-size: 26rpx;
  line-height: 32rpx;
  text-overflow: ellipsis;
  overflow: hidden;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  color: #a6a6a6;
}

.mask-box {
  width: 100%;
  padding: 40rpx 25rpx;
  background-color: #fff;
  border-radius: 10rpx;
  box-sizing: border-box;
  position: absolute;
  left: 0;
  top: 48rpx;
  z-index: 9999;
}

.item-box {
  width: 100%;
  height: 100%;
  overflow: hidden;
}

.itm {
  float: left;
  width: 220rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  background-color: #f7f7f7;
  color: #4d4d4d;
  margin-right: 20rpx;
  margin-bottom: 20rpx;
  font-size: 24rpx;
  box-sizing: border-box;
  border-radius: 4rpx;
  border: 1px solid #ebebeb;
}

.item-box .add {
  background-color: #ffebe0;
  color: #fb7313;
  border: 1px solid #fb7313;
  box-sizing: border-box;
  line-height: 78rpx;
  border-radius: 4rpx;
}

.itm:nth-child(3n) {
  margin-right: 0;
}
.subBtn {
  width: 450rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  color: #fff;
  font-size: 30rpx;
  border-radius: 8rpx;
  background-color: #fb7313;
  margin: auto;
  margin-top: 60rpx;
}




.rewa {
  width: 550rpx;
  height: 560rpx;
  background-color: #fff;
  border-radius: 10rpx;
  overflow: hidden;
  position: fixed;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
}

.rew_top {
  width: 100%;
  height: 300rpx;
}
 .rew_top image {
  display: block;
  width: 100%;
  height: 100%;
  position: relative;
}
.move{ 
  width: 80rpx;
  height: 80rpx;
  position: absolute;
  right: 0;
  top: 0;
}
.rew_btm {
  width: 100%;
  height: 260rpx;
}

.top_text {
  width: 380rpx;
  font-size: 26rpx;
  color: #999;
  text-align: center;
  margin: auto;
  margin-top: 20rpx;

}

.rouTo {
  width: 306rpx;
  height: 65rpx;
  line-height: 65rpx;
  text-align: center;
  color: #fff;
  background-color: #ff921d;
  font-size: 26rpx;
  border-radius: 10rpx;
  margin: auto;
  margin-top: 35rpx;
}