page {
  background-color: #efefef;
  padding-bottom: 20rpx;
}

.head {
  width: 100%;
  height: 360rpx;
  position: relative;
  overflow: hidden;
}

.head_bg {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  z-index: -2;
}

.head_jf {
  position: absolute;
  top: 50rpx;
  right: 0;
  width: 210rpx;
  height: 40rpx;
}

.jf_num {
  width: 100%;
  height: 88rpx;
  color: #fff;
  margin-top: 105rpx;
  margin-bottom: 60rpx;
  text-align: center;
}

.auto {
  display: inline-block;
}

.auto view {
  display: inline-block;
  text-indent: 35rpx;
}

.num {
  font-size: 88rpx;
  line-height: 88rpx;
}

.head_btn {
  width: 100%;
  height: 76rpx;
  display: flex;
}

.head_btn>view {
  width: 50%;
  padding: 0 25rpx;
  box-sizing: border-box;
}

.btn_ {
  width: 246rpx;
  height: 76rpx;
  line-height: 74rpx;
  border: 1px solid #fff;
  color: #fff;
  text-align: center;
  font-size: 28rpx;
  border-radius: 39rpx;
}

.list {
  width: 100%;
  padding: 0 30rpx;
  box-sizing: border-box;
}

.item {
  width: 50%;
  float: left;
  font-size: 24rpx;
  padding-right: 15rpx;
  box-sizing: border-box;
  margin-top: 20rpx;
}

.item:nth-child(2n) {
  padding-right: 0;
  padding-left: 15rpx;
}

.img_top {
  width: 330rpx;
  height: 330rpx;
  background-color: #e5e5e5;
}

.img_top image {
  width: 100%;
  height: 100%;
}

.item_name {
  width: 100%;
  height: 40rpx;
  font-size: 24rpx;
  line-height: 60rpx;
  background-color: #fff;
  padding: 0 20rpx;
  box-sizing: border-box;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.item_btm {
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  background-color: #fff;
  padding: 0 20rpx;
  box-sizing: border-box;
}

.item_btm .lt {
  color: #333;
}

.item_btm .rt {
  color: #fb7313;
}

.shop_box {
  width: 550rpx;
  height: 565rpx;
  position: fixed;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  background-color: #fff;
  border-radius: 10rpx;
  overflow: hidden;
}

.shop_top {
  width: 100%;
  height: 285rpx;
  position: relative;
}

.shop_top .bg {
  width: 100%;
  height: 100%;
}

.shop_top .bg2 {
  width: 80rpx;
  height: 80rpx;
  position: absolute;
  right: 0;
  top: 0;
}

.shop_b {
  width: 100%;
  height: 280rpx;
}

.shop_text {
  font-size: 24rpx;
  line-height: 40rpx;
  color: #afafaf;
  width: 260rpx;
  margin: auto;
  text-align: center;
  margin-top: 50rpx;
  margin-bottom: 50rpx;
}

.shop_b .btn {
  width: 310rpx;
  height: 65rpx;
  line-height: 65rpx;
  text-align: center;
  color: #fff;
  border-radius: 8rpx;
  background-color: #ff921d;
  margin: auto;
  font-size: 28rpx;
}
