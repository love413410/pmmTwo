page {
  background-color: #efefef;
}

.head {
  width: 100%;
  padding: 46rpx;
  box-sizing: border-box;
  display: flex;
  background-color: #fff;
  position: relative;
}

.imgBox {
  width: 128rpx;
  height: 128rpx;
  margin-right: 30rpx;
}

.top_ {
  height: 65rpx;
  overflow: hidden;
}

.top_ .lt {
  margin-right: 15rpx;
}

.name {
  font-size: 38rpx;
  font-weight: 700;
  line-height: 75rpx;
  color: #1a1a1a;
}

.rz {
  font-size: 24rpx;
  height: 65rpx;
  line-height: 65rpx;
  color: #808080;
}
.rzAdd{
  color: #fb7313;
}

.rz-h {
  font-size: 24rpx;
  height: 65rpx;
  line-height: 65rpx;
  color: #fb7313;
  margin-right: 15rpx;
}

.renzImg {
  line-height: 75rpx;
}

.vipImg {
  width: 40rpx;
  height: 46rpx;
  margin-top: 15rpx;
}

.renzImg image, .vipImg image {
  width: 100%;
  height: 100%;
}
.sign{
  width: 165rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  color: #fb7313;
  font-size: 24rpx;
  border: 1px solid #fedac1;
  border-radius: 30rpx;
  position: absolute;
  top: 60rpx;
  right: 40rpx;
}   

.neck {
  width: 100%;
  height: 124rpx;
  background-color: #fec92f;
  display: flex;
  position: relative;
}

.neck .mbg {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
}

.neck .line {
  position: absolute;
  left: 50%;
  top: 14rpx;
  width: 2px;
  height: 96rpx;
  margin-left: -1px;
}

.neck-item {
  width: 50%;
  height: 100%;
  padding-left: 50rpx;
  padding-right: 30rpx;
  box-sizing: border-box;
  color: #fff;
  font-size: 28rpx;
  z-index: 3;
  position: relative;
}

.to {
  width: 100%;
  height: 62rpx;
  line-height: 96rpx;
  position: relative;
}

.to-b {
  font-size: 20rpx;
  line-height: 36rpx;
}

.to image {
  position: absolute;
  bottom: 0;
  width: 62rpx;
  height: 30rpx;
  margin-left: 10rpx;
}

.to-img {
  width: 12rpx;
  height: 21rpx;
  position: absolute;
  right: 30rpx;
  top: 52rpx;
}

.list {
  width: 100%;
  padding-left: 30rpx;
  box-sizing: border-box;
  background-color: #fff;
  margin: 10rpx 0;
}

.item {
  width: 100%;
  height: 95rpx;
  line-height: 95rpx;
  border-bottom: 1px solid #efefef;
  font-size: 28rpx;
  display: flex;
  justify-content: space-between;
}

.pic {
  width: 26rpx;
  height: 26rpx;
  margin-right: 30rpx;
}

.pic image {
  width: 100%;
  height: 100%;
}

