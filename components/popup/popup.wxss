.mask {
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  top: 0;
  background: rgba(0, 0, 0, 0.5);
  z-index: 90;
  overflow: hidden;
  box-sizing: border-box;
}
.box {
  width: 550rpx;
  height: 560rpx;
  position: fixed;
  left:50%;
  top: 50%;
  transform: translate(-50%,-50%);
  position: relative;
}
.pic {
  width: 100%;
  height: 100%;
}
.canc{
  width: 60rpx;
  height: 60rpx;
  position: absolute;
  right: 0;
  top: 0;
}
.cancel{
  width: 200rpx;
  height: 70rpx;
  position: absolute;
  bottom: 30rpx;
  left: 55rpx;
}
.lognBtn{
  width: 200rpx;
  height: 70rpx;
  position: absolute;
  bottom: 32rpx;
  right: 56rpx;
  opacity: 0;
}
