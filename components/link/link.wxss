.macks{
  width: 100%;
  background-color: rgba(0, 0, 0, 0.5);
  position: fixed;
  left: 0;
  top: 0;
  z-index: 9999;
}
.time{
  width: 100%;
  height: 290px;
  position: fixed;
  left: 0;
  bottom: 0;
  background-color: #fff;
}
.tle{
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  font-size: 32rpx;
  border-bottom: 1px solid #e5e5e5;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  padding: 0 40rpx;
  font-weight: 600;
}
.tle view{
  flex: 1;
}
.righ{
  color: #ef7e20;
  text-align: right;
}
picker-view{
  width: 100%;
  height: 400rpx;
}
picker-view-column{
  text-align: center;
}
picker-view-column view{
  line-height: 50px;
}