.lt {
  float: left;
}

.rt {
  float: right;
}

/* 三角形 以及一些其他奇怪的教*/

.tri {
  display: inline-block;
  width: 0;
  height: 0;
  border-width: 15rpx;
  border-style: solid;
  border-color: #999 transparent transparent transparent;
}

.cro_left_top, .cro_right_top, .cro_left_bottom, .cro_right_bottom {
  position: absolute;
  width: 15px;
  height: 15px;
  border: 1px solid #efefef;
  z-index: 1;
  background: #efefef;
}

.cro_left_top {
  top: -1px;
  left: -1px;
  border-radius: 0px 0px 26px 0px;
  border-bottom: 1px solid #efefef;
  border-right: 1px solid #efefef;
}

.cro_right_top {
  top: -1px;
  right: -1px;
  border-radius: 0px 0px 0px 26px;
  border-bottom: 1px solid #efefef;
  border-left: 1px solid #efefef;
}

.cro_left_bottom {
  left: -1px;
  bottom: -1px;
  border-radius: 0px 26px 0px 0px;
  border-top: 1px solid #efefef;
  border-right: 1px solid #efefef;
}

.cro_right_bottom {
  right: -1px;
  bottom: -1px;
  border-radius: 26px 0px 0px 0px;
  border-top: 1px solid #efefef;
  border-left: 1px solid #efefef;
}

/* 初始化 */

.sup {
  position: relative;
}

.sup::after {
  content: '2';
  position: absolute;
  right: -17rpx;
  top: -5rpx;
}

/* 遮罩层 */

.mask {
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  top: 0;
  background: rgba(0, 0, 0, 0.5);
  z-index: 999;
  overflow: hidden;
  box-sizing: border-box;
}

.imgBox image{
  display: block;
  width: 100%;
  height: 100%;
}




/* 多个页面用 */
.foot {
  width: 100%;
  background-color: #efefef;
  padding: 35rpx;
  box-sizing: border-box;
  font-size: 22rpx;
  line-height: 57rpx;
}

.foot-head {
  width: 100%;
  text-align: center;
  font-size: 24rpx;
  color: #666666;
}

.line {
  position: relative;
}

.line::after {
  position: absolute;
  left: -35rpx;
  top: 50%;
  content: "";
  width: 35rpx;
  height: 2rpx;
  background-color: #000;
}
.line::before {
  position: absolute;
  right: -45rpx;
  top: 50%;
  content: ""; 
  width: 35rpx;
  height: 2rpx;
  background-color: #000;
}



/* 支付弹窗 */
.pay_box {
  width: 600rpx;
  height: 565rpx;
  position: fixed;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  background-color: #fff;
  border-radius: 10rpx;
  padding: 30rpx;
  box-sizing: border-box;
}

.pay_head {
  height: 55rpx;
  line-height: 55rpx;
  width: 100%;
  font-size: 32rpx;
  text-align: center;
  font-weight: 600;
  overflow: hidden;
}

.pay_img {
  width: 35rpx;
  height: 35rpx;
  float: left;
  margin-top: 10rpx;
}

.pay_text {
  text-align: center;
  font-size: 32rpx;
  margin: 50rpx 0;
  font-weight: 700;
}

.pay_texts {
  margin-bottom: 0;
}

.pay_type {
  width: 100%;
  height: 110rpx;
  border-top: 1px solid #efefef;
  font-size: 24rpx;
}

.pay_lt {
  float: left;
  line-height: 110rpx;
}

.pay_rt {
  float: right;
  line-height: 110rpx;
}

.pay_lq {
  float: left;
  width: 32rpx;
  height: 32rpx;
  margin-top: 39rpx;
}

.pay_lqt {
  margin: 0 10rpx;
}
.pay_contents{
  width: 100%;
  box-sizing: border-box;
  display: flex;
  justify-content: space-around;
  align-items: center;
  margin-top: 50rpx;
}

.pay_iptbox{
  flex: 1;
  height: 80rpx;
  border:1rpx solid #ddd;
  box-sizing: border-box;
  display: flex;
  justify-content: center;
  align-items: center;
  text-align: center;
}
.pay_ipt_s{
  width: 0;
  height: 0;
}





